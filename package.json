{
    "name": "music",
    "version": "2.0.1",
    "main": "main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "electron .",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "com.example.music",
        "directories": {
            "output": "dist"
        },
        "mac": {
            "icon": "favicon.icns",
            "category": "public.app-category.productivity"
        },
        "win": {
            "icon": "favicon.ico",
            "target": "nsis"
        },
        "linux": {
            "icon": "favicon.png"
        }
    },
    "keywords": ["Medzy Music", "music app", " music streaming", "playlists", "discover music", "personalized recommendations"],
    "author": "Medzy Amara",
    "license": "ISC",
    "description": "Explore Medzy Music - Your ultimate destination for discovering and enjoying music across genres. Discover new tracks, create playlists, and enjoy personalized recommendations.",
    "devDependencies": {
        "electron-builder": "^24.13.3",
        "electron": "^30.0.8"
    }
}